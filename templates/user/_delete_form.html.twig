<form method="post" action="{{ path('user_delete', {'id': app.user.id}) }}" onsubmit="return confirm('Vous etes dur de vouloir supprimer cet utilisateur ?');">

    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ app.user.id) }}">

    <button class="btn btn-primary block full-width m-b bg-danger">Supprimer le Profil</button>
</form>
